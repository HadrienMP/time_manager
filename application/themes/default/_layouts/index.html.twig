<!DOCTYPE html><html>	<head>		<!-- Meta data -->		<meta charset="utf-8" />		<title>{% block title %}Puncher{% endblock %}</title>                <meta name="viewport" content="width=device-width, initial-scale=1" />                <link rel="shortcut icon" href="/images/favicon.ico" type="image/x-icon" />        <link rel="stylesheet" href="/css/screen.css">                <script type="text/javascript" src="/js/lib/jquery-1.9.0.js"></script>        <script type="text/javascript" src="/js/utils.js"></script>		{% block head %}		{% endblock %}	</head>	<body>		<div id="wrapper" class="{{checked_in}}">                    <div id="machine">                <div id="puncher-container">                    <a href="/manager/punch">&#xF011;</a>                </div>                <div id="tools-container">                    <ul id="tools">                        <li class="{{ active == "stats" ? "active" : "" }}"><a href="stats">Statistiques</a></li>                        <li class="{{ active == "punches" ? "active" : "" }}"><a href="punches">Pointages</a></li>                        <li class="{{ active == "preferences" ? "active" : "" }}"><a href="preferences">Paramètres</a></li>                        <li class="{{ active == "data" ? "active" : "" }}"><a href="data">Données</a></li>                    </ul>                </div>            </div>                        <div id="content">                {% block content %}                {% endblock %}            </div>                        <ul id="navigation">                <li><a href="account">Compte</a></li>                <li><a href="info">Informations</a></li>                <li><a href="/auth/logout">Déconnexion</a></li>            </ul>		</div>	</body></html>