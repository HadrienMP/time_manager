<!DOCTYPE html><html>	<head>		<!-- Meta data -->		<meta charset="utf-8" />		<title>{% block title %}Puncher{% endblock %}</title>                <meta name="viewport" content="width=device-width, initial-scale=1" />                <link rel="shortcut icon" href="/images/favicon.ico" type="image/x-icon" />        <link rel="stylesheet" href="/css/screen.css">                <script type="text/javascript" src="/js/lib/jquery-1.9.0.js"></script>        <script type="text/javascript" src="/js/utils.js"></script>		{% block js %}		{% endblock %}	</head>	<body>		<div id="wrapper" class="{{checked_in}}">                    <div id="machine">                <div id="puncher-container">                    <!-- The puncher button -->                    <section id="puncher-section">                        <!-- Knob that represents the % of time spent for the day -->                        <input id="knob" data-width="100" data-height="100" data-inline="false" data-thickness=".1"                                data-skin="tron" data-displayInput="false" style="display: none" />                        <!-- The real puncher button -->                        <a id="puncher-button" class="box" href="/manager/punch" ><span>&#xF011;</span></a>                    </section>                </div>                <div id="tools-container">                    <ul id="tools">                        <li><a href="stats" class="{{ active == "stats" ? "active" : "" }}">Statistiques</a></li><!-- whitespace                     --><li><a href="punches" class="{{ active == "punches" ? "active" : "" }}">Pointages</a></li><!-- whitespace                     --><li><a href="preferences" class="{{ active == "preferences" ? "active" : "" }}">Paramètres</a></li><!-- whitespace                     --><li><a href="data" class="{{ active == "data" ? "active" : "" }}">Données</a></li>                    </ul>                </div>            </div>                        <div id="content">                {% block content %}                {% endblock %}            </div>                        <ul id="navigation">                <li><a href="account">Compte</a></li><!-- whitespace             --><li><a href="info">Informations</a></li>            </ul>		</div>	</body></html>