<!DOCTYPE html><html>	<head>		<!-- Meta data -->		<meta charset="utf-8" />		<title>{% block title %}Puncher{% endblock %}</title>                <meta name="viewport" content="width=device-width, initial-scale=1" />                <link rel="shortcut icon" href="/images/favicon.ico" type="image/x-icon" />        <link rel="stylesheet" media="screen" href="/css/screen.css" />        <!--[if lt IE 9]>		<link rel="stylesheet" href="/css/ie.css">		<![endif]-->                <script type="text/javascript" src="/js/lib/jquery-1.9.1.js"></script>        <script type="text/javascript" src="/js/lib/jquery-ui-1.10.3.custom.min.js"></script>        <script type="text/javascript" src="/js/utils.js"></script>        <script src="/js/lib/jquery.knob.js"></script>        <script src="/js/knob-init.js"></script>        <script type="text/javascript">            $(function() {                $("#knob").knob({                    {% if checked_in %}                        {% if overtime_absolute %}                            "fgColor":"#FF0000",                        {% elseif overtime %}                            "fgColor":"#FF9500",                        {% else %}                            "fgColor":"#0095FF",                        {% endif %}                    {% else %}                        "fgColor":"#aaa",                    {% endif %}                    draw : tronDraw                });                $("#knob").val({{ ratio }});                $("#knob").change();            });        </script>		{% block head %}		{% endblock %}	</head>	<body>		<div id="wrapper" class="{{checked_in}} {{overtime}} {{overtime_absolute}}">                    <div id="machine" class="has_light">                <div id="puncher-container">                    <!-- The puncher button -->                    <section id="puncher-section">                        <!-- Knob that represents the % of time spent for the day -->                        <input id="knob" data-width="130" data-height="130" data-inline="false" data-thickness=".1"                                data-skin="tron" data-displayInput="false" style="display: none" value="" />                        <!-- The real puncher button -->                        <a id="puncher-button" class="box has_light" href="/manager/punch" title="{{ floor(ratio) }}%"><span>&#xF011;</span></a>                    </section>                </div>                <div id="tools-container">                    <ul id="tools">                        <li><a href="/stats" id="stats-button" class="{{ active == "stats" ? "active" : "" }}">Statistiques</a></li><!-- whitespace                     --><li><a href="/punches" id="punches-button" class="{{ active == "punches" ? "active" : "" }}">Pointages</a></li><!-- whitespace                     --><li><a href="/preferences" id="pref-button" class="{{ active == "preferences" ? "active" : "" }}">Paramètres</a></li><!-- whitespace                     --><li><a href="/data" id="data-button" class="{{ active == "data" ? "active" : "" }}">Données</a></li>                    </ul>                </div>            </div>                        <div id="content">                {% block content %}                {% endblock %}            </div>                         <ul id="navigation">        	            <!--[if lt IE 10]>				<li class="left" style="float: left; padding: 10px; color: #CCC">					L'auteur de ce site vous déconseille l'utilisation d'internet explorer en version inférieure à 10.				</li>				<![endif]--><!-- whitespace				{% for url, name in navigation_items %}                    --><li><a href="{{ url }}">{{ name }}</a></li><!-- whitespace                {% endfor %}                -->            </ul>		</div>        <script>		  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){		  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),		  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)		  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');				  ga('create', 'UA-47813178-1', 'hadrienmp.fr');		  ga('send', 'pageview');				</script>	</body></html>